<ul class="media-list">
    <% microposts.each do |micropost| %>
      <li class="media">
        <div class="media-left">
          <img class="media-object img-rounded" src="<%= gravatar_url(user, { size: 50 }) %>" alt="">
        </div>
        <div class="media-body">
          <div>
            <%= micropost.content %>
          </div>
        </div>
      </li>
    <% end %>
  </ul>
<%= paginate microposts %>